{
  "name": "solzu",
  "version": "1.0.18",
  "author": "Daniel Montilla",
  "description": "collections of useful typescript stuff",
  "homepage": "https://github.com/DanielMontilla/solzu#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielMontilla/solzu"
  },
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/DanielMontilla/solzu/issues"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "prettier": "^3.2.5",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "type": "./dist/index.d.cts"
      }
    },
    "./@maybe": {
      "import": {
        "default": "./dist/maybe/index.js",
        "types": "./dist/maybe/index.d.ts"
      },
      "require": {
        "default": "./dist/maybe/index.cjs",
        "types": "./dist/maybe/index.d.cts"
      }
    },
    "./@result": {
      "import": {
        "default": "./dist/result/index.js",
        "types": "./dist/result/index.d.ts"
      },
      "require": {
        "default": "./dist/result/index.cjs",
        "types": "./dist/result/index.d.cts"
      }
    }
  },
  "files": [
    "dist"
  ]
}