{
  "author": "Daniel Montilla",
  "name": "solzu",
  "scripts": {
    "build": "tsc && vite build",
    "test": "vitest"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./@result": {
      "import": "./dist/result/scoped.js",
      "require": "./dist/result/scoped.cjs",
      "types": "./dist/result/scoped.d.ts"
    },
    "./@maybe": {
      "import": "./dist/maybe/scoped.js",
      "require": "./dist/maybe/scoped.cjs",
      "types": "./dist/maybe/scoped.d.ts"
    }
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2",
    "vite-plugin-dts": "^3.9.0",
    "prettier": "^3.2.5",
    "@types/node": "^20.12.7"
  },
  "files": [
    "dist"
  ],
  "description": "collections of usefull typescript stuff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielMontilla/solzu"
  },
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "license": "ISC",
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/DanielMontilla/solzu/issues"
  },
  "homepage": "https://github.com/DanielMontilla/solzu#readme"
}